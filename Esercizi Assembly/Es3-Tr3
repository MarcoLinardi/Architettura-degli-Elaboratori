.data

spazio : .word 1000
tempo : .word 30

.text
.globl main
main:

# Carica gli indirizzi delle variabili nella memoria nei registri $t1 e $t2
la $t1, spazio
la $t2, tempo

# Carica la distanza e il tempo nei registri $s0 e $s1
lw $s0, 0($t1)
lw $s1, 0($t2)

#Calcolo la velocit√† con "spazio/tempo"
div $s0,$s1

#Memorizza il risultato nel registro $t0
mflo $t0

li $v0,10
syscall





