.data
string: .asciiz "trentatre trentini entrarorono a trento tutti e trentatre trotterellando"

.text
.globl main
main:
	la $t0,string
	la $t3,string
	li $t1,32
	
Loop:
	lb $t2,0($t0)
	beq $t2,$zero,Print
	beq $t2,$t1,Jump_char
	addi $t0,$t0,1
	sb $t2,0($t3)
	addi $t3,$t3,1
	j Loop
	
Jump_char:
	addi $t0,$t0,1
	j Loop
	
Print:
	sb $zero,0($t3)
	li $v0,4
    	la $a0,string
    	syscall
    	li $v0,10
    	syscall