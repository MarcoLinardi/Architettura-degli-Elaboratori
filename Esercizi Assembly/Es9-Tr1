.data

coppia: .asciiz " Coppia"
no_coppia: .asciiz " Non Ã¨ una coppia"
spazio: .asciiz " "

.text
.globl main
main:
	li $a1,6
	li $v0,42
	syscall
	move $t0,$a0
	li $a1,6
	li $v0,42
	syscall
	move $t1,$a0
	
	move $a0,$t0
	li $v0,1
	syscall
	li $v0,4
	la $a0,spazio
	syscall
	move $a0,$t1
	li $v0,1
	syscall
	
	beq $t0,$t1,Coppia
	j No_coppia
	
Coppia:
	li $v0,4
	la $a0,coppia
	syscall
	li $v0,10
	syscall
	
No_coppia:
	li $v0,4
	la $a0,no_coppia
	syscall
	li $v0,10
	syscall


