.data

batman : .word 10
robin : .word 50

.text
.globl main
main: 

lw $t1,batman
lw $t2,robin
slt $t0,$t1,$t2      #se $t1<$t2 setta $t0 a 1, senno setta $t0 a 0

li $v0,1		#Codice per la stampa a video
move $a0,$t0
syscall		#Esegue la stampa a video

li $v0,10	#Codice per terminare il programma
syscall		#Esegue la terminazione del programma