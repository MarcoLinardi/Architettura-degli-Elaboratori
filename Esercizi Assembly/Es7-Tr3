.data
x1: .word 5
x2: .word 10
y1: .word 6
y2: .word 4
distanza: .float 0

.text
.globl main
main:
	lw $t0,x1
	lw $t1,x2
	sub $t4,$t0,$t1
	mul $t5,$t4,$t4
	mtc1 $t5,$f0
	cvt.s.w $f0,$f0
	
	lw $t2,y1
	lw $t3,y2
	sub $t6,$t2,$t3
	mul $t7,$t6,$t6
	mtc1 $t7,$f1
	cvt.s.w $f1,$f1
	
	add.s $f0,$f0,$f1
	sqrt.s $f0,$f0
	
	s.s $f0,distanza
	li $v0,2
	l.s $f12,distanza
	syscall
	li $v0,10
	syscall
	
	
	
	