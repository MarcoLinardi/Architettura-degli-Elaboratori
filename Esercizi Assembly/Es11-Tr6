.data
stringa1: .asciiz "Gatto"
stringa2: .asciiz "Matto"
stringa_finale: .space 100

.text
.globl main 
main:
	la $t0,stringa1
	la $t1,stringa2
	la $t2,stringa_finale
	
Loop1:
	lb $t3,0($t0)
	beq $t3,$zero,Loop2
	sb $t3,0($t2)
	addi $t0,$t0,1
	addi $t2,$t2,1
	j Loop1
	
Loop2:
	lb $t4,0($t1)
	beq $t4,$zero,Print
	sb $t4,0($t2)
	addi $t1,$t1,1
	addi $t2,$t2,1
	j Loop2
		
Print:
	sb $zero,0($t2)
	li $v0,4
	la $a0,stringa_finale
	syscall
	li $v0,10
	syscall