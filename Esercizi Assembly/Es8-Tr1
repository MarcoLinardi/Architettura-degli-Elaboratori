.data

input: .asciiz "Inserisci un numero: "
output: .asciiz "Il risultato Ã¨: "

.text
.globl main 
main:
	li $v0,4
	la $a0,input
	syscall
	li $v0,5
	syscall
	move $t0,$v0
	
	li $v0,4
	la $a0,input
	syscall
	li $v0,5
	syscall
	move $t1,$v0
	
	add $t2,$t0,$zero
	
Moltiplica:
	beqz $t1,Stampa
	mul $t2,$t2,$t0
	subi $t1,$t1,1
	j Moltiplica
	
Stampa:
	li $v0,4
	la $a0,output
	syscall
	move $a0,$t2
	li $v0,1
	syscall
	li $v0,10
	syscall
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	