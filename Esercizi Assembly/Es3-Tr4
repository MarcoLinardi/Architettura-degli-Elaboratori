.data

Celsius : .word 38
Farhenheit : .word 0

.text
.globl main
main:

# Carico il valore Celsius dalla memoria e moltiplico per 9
lw $t1, Celsius 
li $t2,9
mul $t1,$t1,$t2

# Moltiplico per 5 e salvo il risultato in $t1
li $t2, 5
div $t1,$t2
mflo $t1

# Addiziono 32 per completare la formula di conversione
li $t2, 32
add $t0,$t1, $t2

# Carico il risultato in memoria
sw $t0, Farhenheit

# Fine del programma (jump register to return address)
jr $ra