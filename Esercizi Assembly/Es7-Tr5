.data

input: .asciiz "Inserisci un numero: "
denominatore: .float 1
limite: .float 5

.text
.globl main 
main:
	li $v0,4
	la $a0,input
	syscall
	li $v0,5
	syscall
	move $t0,$v0
	
	blez $t0,Termina
	mtc1 $t0,$f0
	cvt.s.w $f0,$f0
	
	lwc1 $f2,denominatore
	lwc1 $f3,limite
	
Loop_Somma:
	
	
	  
	    
	      
	        
Termina: 
	li $v0,10
	syscall  
